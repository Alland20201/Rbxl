						<string name="Name">InsertBoard</string>
						<ProtectedString name="Source">local skateboardId = 23154619 

local Tool = script.Parent;

function insert()
&#9;--This call will cause a &quot;wait&quot; until the data comes back
&#9;--put the board model in lighting!!!
&#9;local root = game.Lighting.Board:clone()

&#9;--Continue the insert process
&#9;root.Name = &quot;InsertedObject&quot; .. skateboardId

&#9;--if your skateboard uses a mesh, uncomment this code.
&#9;--the mesh MUST be in your tool&apos;s handle part.
&#9;local Mesh = Tool.Handle.Mesh:clone()
&#9;Mesh.Parent = root.SkateboardPlatform
&#9;
&#9;root.Parent = game.Workspace

&#9;-- for some reason copying from lighting fucks up the position.
&#9;root[&quot;Left Front&quot;].Position = Vector3.new(2.5, 75, -1.5)
&#9;root[&quot;Left Rear&quot;].Position = Vector3.new(-2.5, 75, -1.5)
&#9;root[&quot;Right Front&quot;].Position = Vector3.new(-2.5, 75, 1.5)
&#9;root[&quot;Right Rear&quot;].Position = Vector3.new(2.5, 75, 1.5)

&#9;local t = game.Players.LocalPlayer.Character:FindFirstChild(&quot;Torso&quot;)

&#9;root:MoveTo(t.Position + t.CFrame.lookVector * 8)
&#9;Tool.Handle.Drop:Play()
end

enabled = true
function onButton1Down(mouse)
&#9;if not enabled then
&#9;&#9;return
&#9;end

&#9;enabled = false


&#9;insert()
&#9;wait(.01)

&#9;Tool:Remove()


end

function onEquippedLocal(mouse)

&#9;if mouse == nil then
&#9;&#9;print(&quot;Mouse not found&quot;)
&#9;&#9;return 
&#9;end

&#9;mouse.Icon = &quot;rbxasset://../../../shareddata/assets/22858207&quot;
&#9;mouse.Button1Down:connect(function() onButton1Down(mouse) end)
end


Tool.Equipped:connect(onEquippedLocal)</ProtectedString>
